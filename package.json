{
  "name": "PeacockUI",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/peacockui.js",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint . --ext .tsx,.js,.ts --fix",
    "format": "prettier --write \"./**/*.{html,tsx,js,json,md,less}\"",
    "prepare": "husky install",
    "build": "pnpm build:components",
    "build:all": "vite build",
    "build:components": "esno ./scripts/build.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-react-pages": "^4.0.2"
  },
  "devDependencies": {
    "@iconify-json/ic": "1.1.4",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "prettier": "^2.8.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "terser": "5.4.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unocss": "0.44.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vitepress": "1.0.0-alpha.4"
  }
}
